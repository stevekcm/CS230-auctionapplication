<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="artatawe.controller.LoginController">
    <children>
        <Pane layoutX="7.0" layoutY="7.0" prefHeight="486.0" prefWidth="686.0">
            <children>
                <Button fx:id="loginBtn" layoutX="358.0" layoutY="261.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="23.0" prefWidth="76.0" text="Login" />
                <TextField fx:id="username" layoutX="275.0" layoutY="202.0" />
                <Label layoutX="323.0" layoutY="156.0" text="Username :" textFill="#97907c" />
                <Label layoutX="40.0" layoutY="26.0" text="Artatawe" textFill="#f5a823">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Label>
                <Label fx:id="loginMessage" layoutX="308.0" layoutY="236.0"  />
            <Button fx:id="registerBtn" layoutX="275.0" layoutY="261.0" mnemonicParsing="false" onAction="#registerAction" prefHeight="23.0" prefWidth="76.0" text="Register" />
            </children>
        </Pane>
    </children>
</AnchorPane>
