<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>

<AnchorPane fx:id="rootPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="artatawe.controller.FavArtworkController">
    <children>
        <TableView layoutX="170.0" layoutY="74.0" prefHeight="400.0" prefWidth="505.0">
            <columns>
                <TableColumn prefWidth="139.0" text="Photo" />
                <TableColumn prefWidth="151.0" text="Artist" />
                <TableColumn prefWidth="128.0" text="Year" />
                <TableColumn minWidth="1.0" prefWidth="123.0" />
            </columns>
        </TableView>
    </children>
    <AnchorPane prefHeight="500.0" prefWidth="158.0" style="-fx-background-color: #1D2026;">
        <Label layoutX="39.0" layoutY="30.0" text="Artatawe" textFill="aliceblue">
            <font>
                <Font name="System Bold" size="20.0" />
            </font></Label>
        <Button fx:id="homeBtn" layoutX="6.0" layoutY="167.0" mnemonicParsing="false" onAction="#homeAction" prefHeight="23.0" prefWidth="146.0" style="-fx-background-color: #1D2026;" text="Home" textFill="WHITE" />
        <!-- Favourite Artwork Button: -->
        <Button fx:id="editProfileBtn" layoutX="20.0" layoutY="203.0" mnemonicParsing="false" onAction="#editProfileAction" prefHeight="23.0" prefWidth="127.0" style="-fx-background-color: #1D2026;" text="Edit Profile" textFill="WHITE" />
        <!-- Favourite User Button: -->
        <Button fx:id="favUserBtn" layoutX="22.0" layoutY="239.0" mnemonicParsing="false" onAction="#favUserAction" prefHeight="23.0" prefWidth="115.0" style="-fx-background-color: #1D2026;" text="Favourite User" textFill="WHITE" />
        <ImageView fx:id="smallAvatar" fitHeight="46.0" fitWidth="49.0" layoutX="55.0" layoutY="92.0" pickOnBounds="true" preserveRatio="true" />
    </AnchorPane>
</AnchorPane>
